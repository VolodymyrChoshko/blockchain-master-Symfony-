<link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico') }}" />
<link rel="apple-touch-icon" href="{{ asset('images/appicon.png') }}">
<link
  rel="stylesheet"
  href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
/>
<link href="{{ asset('css/jquery.fancybox.min.css') }}" rel="stylesheet" type="text/css" />
<link href="{{ asset('build/style.css') }}" rel="stylesheet" type="text/css" />
<link href="{{ asset('build/builder.css') }}" rel="stylesheet" type="text/css" />
{% if assets.css is defined %}
  {% for a in assets.css %}
    <link href="{{ asset(a) }}" rel="stylesheet" type="text/css" />
  {% endfor %}
{% endif %}

<script src="https://polyfill.io/v3/polyfill.min.js?features=ResizeObserver"></script>
<script src="{{ asset('https://code.jquery.com/jquery-3.4.1.min.js') }}"></script>
{% if request.route.name starts with "admin_" %}
  <link href="{{ asset('build/admin.css') }}" rel="stylesheet" type="text/css" />
  <script src="{{ asset('https://code.jquery.com/jquery-3.4.1.min.js') }}"></script>
  <script>
    window.jConfirmSimple = function(msg) {
      return new Promise(function(resolve) {
        if (confirm(msg)) {
          return resolve(true);
        }
      });
    }
  </script>
  {% if socket is defined %}
    <script>
      window.socket = {{ socket|json_encode|raw }};
    </script>
  {% endif %}
  <script src="//twitter.github.io/typeahead.js/releases/latest/typeahead.bundle.min.js"></script>
  <script src="{{ asset('build/admin.js') }}"></script>
{% endif %}
<script src="https://js.stripe.com/v2/"></script>
{% if assets.js is defined %}
  {% for a in assets.js %}
    <script src="{{ asset(a) }}"></script>
  {% endfor %}
{% endif %}
